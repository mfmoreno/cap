# Virtuoso Configuration
# Host and credentials for the Virtuoso triple store
VIRTUOSO_HOST=virtuoso
VIRTUOSO_PORT=8890
VIRTUOSO_USER=dba
VIRTUOSO_PASSWORD=your_secret_password_here
SPARQL_UPDATE=true

# Cardano Knowledge Graph Configuration
# URI for the Cardano ontology graph in Virtuoso
CARDANO_GRAPH=http://www.mobr.ai/ontologies/cardano

# OpenTelemetry Configuration
# OTLP exporter endpoint for telemetry data
OTEL_EXPORTER_OTLP_ENDPOINT=http://jaeger:4317

# Service name for telemetry identification
OTEL_SERVICE_NAME=cap

# postgresql
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secret_password_here
POSTGRES_DB=cap
DATABASE_URL=postgresql+psycopg2://postgres:your_secret_password_here@postgres:5432/cap

# Logging
ENABLE_TRACING=True
LOG_LEVEL=INFO
ETL_METRICS_ENABLED=True

# ETL configuration
ETL_AUTO_START=False
ETL_BATCH_SIZE=1000
ETL_SYNC_INTERVAL=300
ETL_CONTINUOUS=True
ETL_PROGRESS_GRAPH=http://www.mobr.ai/ontologies/cardano/metadata
ETL_PARALLEL_WORKERS=1

# CAP configuration
CAP_HOST=localhost
CAP_PORT=8000
PUBLIC_BASE_URL=https://cap.mobr.ai

# llm
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL_NAME=mobr/cap

# Mailing
RESEND_API_KEY=your_key_here